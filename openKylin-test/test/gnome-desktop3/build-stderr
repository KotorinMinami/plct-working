+ mode=dynamic
+ getopt -o  --long static -n debian/tests/build --
+ getopt_temp= --
+ eval set --  --
+ set -- --
+ true
+ shift
+ break
+ mktemp -d
+ WORKDIR=/tmp/tmp.ctZQbUT0WW
+ trap rm -rf /tmp/tmp.ctZQbUT0WW 0 INT QUIT ABRT PIPE TERM
+ cd /tmp/tmp.ctZQbUT0WW
+ cat
+ cflags=
+ pcflags=
+ pkg-config --cflags --libs gnome-desktop-3.0
+ gcc -o dynamic gnome-desktop.c -pthread -I/usr/include/gnome-desktop-3.0 -I/usr/include/gtk-3.0 -I/usr/include/at-spi2-atk/2.0 -I/usr/include/at-spi-2.0 -I/usr/include/dbus-1.0 -I/usr/lib/riscv64-linux-gnu/dbus-1.0/include -I/usr/include/gtk-3.0 -I/usr/include/gio-unix-2.0 -I/usr/include/cairo -I/usr/include/pango-1.0 -I/usr/include/fribidi -I/usr/include/harfbuzz -I/usr/include/atk-1.0 -I/usr/include/cairo -I/usr/include/pixman-1 -I/usr/include/uuid -I/usr/include/freetype2 -I/usr/include/libpng16 -I/usr/include/gdk-pixbuf-2.0 -I/usr/include/libmount -I/usr/include/blkid -I/usr/include/glib-2.0 -I/usr/lib/riscv64-linux-gnu/glib-2.0/include -I/usr/include/gsettings-desktop-schemas -lgnome-desktop-3 -lgtk-3 -lgdk-3 -lpangocairo-1.0 -lpango-1.0 -lharfbuzz -latk-1.0 -lcairo-gobject -lcairo -lgdk_pixbuf-2.0 -lgio-2.0 -lgobject-2.0 -lglib-2.0
+ echo build (dynamic): OK
+ [ -x dynamic ]
+ ./dynamic
+ echo run (dynamic): OK
+ rm -rf /tmp/tmp.ctZQbUT0WW
