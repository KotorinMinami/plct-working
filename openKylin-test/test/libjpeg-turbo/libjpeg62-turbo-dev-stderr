+ mode=dynamic
+ getopt -o  --long static -n debian/tests/libjpeg62-turbo-dev --
+ getopt_temp= --
+ eval set --  --
+ set -- --
+ true
+ shift
+ break
+ mktemp -d
+ WORKDIR=/tmp/tmp.gcx8u4Fq0t
+ trap rm -rf "$WORKDIR" 0 INT QUIT ABRT PIPE TERM
+ [ -n  ]
+ CROSS_COMPILE=
+ CC=gcc
+ PKG_CONFIG=pkg-config
+ cd /tmp/tmp.gcx8u4Fq0t
+ cat
+ cflags=
+ pcflags=
+ exe=use-libjpeg-dynamic
+ pkg-config --cflags --libs libjpeg
+ gcc -o use-libjpeg-dynamic use-libjpeg.c -ljpeg
+ [ -x use-libjpeg-dynamic ]
+ echo build (dynamic): OK
+ ./use-libjpeg-dynamic
+ echo run (dynamic): OK
+ rm -rf /tmp/tmp.gcx8u4Fq0t
