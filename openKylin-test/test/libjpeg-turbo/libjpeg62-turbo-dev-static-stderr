+ exec debian/tests/libjpeg62-turbo-dev --static
+ mode=dynamic
+ getopt -o  --long static -n debian/tests/libjpeg62-turbo-dev -- --static
+ getopt_temp= --static --
+ eval set --  --static --
+ set -- --static --
+ true
+ mode=static
+ shift
+ continue
+ true
+ shift
+ break
+ mktemp -d
+ WORKDIR=/tmp/tmp.0sC4LBg4j8
+ trap rm -rf "$WORKDIR" 0 INT QUIT ABRT PIPE TERM
+ [ -n  ]
+ CROSS_COMPILE=
+ CC=gcc
+ PKG_CONFIG=pkg-config
+ cd /tmp/tmp.0sC4LBg4j8
+ cat
+ cflags=
+ pcflags=
+ cflags=-static
+ pcflags=--static
+ exe=use-libjpeg-static
+ pkg-config --static --cflags --libs libjpeg
+ gcc -static -o use-libjpeg-static use-libjpeg.c -ljpeg
+ [ -x use-libjpeg-static ]
+ echo build (static): OK
+ ./use-libjpeg-static
+ echo run (static): OK
+ rm -rf /tmp/tmp.0sC4LBg4j8
